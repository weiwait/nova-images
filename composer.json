{
    "name": "weiwait/nova-images",
    "description": "A Laravel Nova field for multiple images by json. Laravel Nova 多图组件，目前仅支持json格式",
    "keywords": [
        "laravel", "nova", "images", "多图", "drop", "裁剪", "多图裁剪"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Weiwait\\NovaImages\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weiwait\\NovaImages\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
